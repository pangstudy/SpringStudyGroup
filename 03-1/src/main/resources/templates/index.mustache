{{>layout/header}}
<h1>웹서비스</h1>
<div class="col-md-12">
    <div class="row">
        <div class="col-md-6">
            <a href="/board/save" role="button" class="btn btn-primary">글쓰기</a>
        </div>
    </div>
    <br>
    <table class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>게시판번호</th>
            <th>제목</th>
            <th>작성일</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#board}}
            <tr>
                <td>{{id}}</td>
                <td><a href="/board/update/{{id}}">{{title}}</a></td>
                <td>{{regDate}}</td>
            </tr>
        {{/board}}
        </tbody>
    </table>
</div>
{{>layout/footer}}
